{
  "name": "testring-runner",
  "displayName": "Testring Runner",
  "description": "",
  "version": "0.0.5",
  "publisher": "xiaomocqk",
  "repository": "https://github.com/xiaomocqk/testring-runner",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Testing"
  ],
  "activationEvents": [
    "workspaceContains:.testringrc.js"
  ],
