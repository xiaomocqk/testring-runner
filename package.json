    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "packages": "vsce package"
  },
  "devDependencies": {
    "@babel/types": "^7.23.3",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@babel/parser": "^7.23.3"
  }
}
